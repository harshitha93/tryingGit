<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android" >

    <include
        layout="@layout/tool_bar_new"
        android:id="@+id/toolbar1"
        >

    </include>


    <android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/drawer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        tools:context=".MainActivity">

        <ScrollView
            android:layout_height="match_parent"
            android:layout_width="match_parent">


        <LinearLayout
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"

            >

            <FrameLayout
                android:id="@+id/frame"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:id="@+id/rel1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp">

                <TextView
                    android:id="@+id/tv1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="2dp"
                    android:text="@string/textview8"
                    android:layout_marginTop="10dp"
                    android:textColor="#000"
                    android:textSize="18sp"/>
                <TextView
                    android:id="@+id/tv2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv1"
                    android:padding="2dp"
                    android:textSize="15sp"
                    android:text="@string/textview9"

                    />


                    <TextView
                        android:id="@+id/tv3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/textview10"
                        android:textColor="#000"
                        android:textSize="15sp"
                        android:layout_marginLeft="280dp"
                        android:padding="10dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true" />

                    </RelativeLayout>



                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/rel1"
                    android:background="#000"
                    android:layout_marginTop="5dp"/>

                <RelativeLayout
                    android:id="@+id/rel2"
                    android:layout_below="@+id/rel1"
                    android:layout_marginTop="10dp"
                    android:layout_margin="5dp"
                    android:textColor="#000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/tv4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="2dp"
                        android:text="@string/textview11"
                        android:textColor="#000"
                        android:textSize="18sp"
                        />

                    <TextView
                        android:id="@+id/tv5"
                        android:layout_below="@+id/tv4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/textview12"
                        android:padding="2dp"
                        android:textSize="15sp"
                        />
                    <TextView
                        android:id="@+id/tv6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/textview10"
                        android:textColor="#000"
                        android:layout_marginLeft="280dp"
                        android:padding="10dp"
                        android:textSize="15sp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true" />


                   </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/rel2"
                        android:background="#000"
                        android:layout_marginTop="5dp"/>

                    <RelativeLayout
                        android:id="@+id/rel3"
                        android:layout_below="@+id/rel2"
                        android:layout_marginTop="10dp"
                        android:textColor="#000"
                        android:layout_margin="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/tv7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="2dp"
                            android:text="@string/textview13"
                            android:textSize="18sp"
                            android:textColor="#000"
                            />

                        <TextView
                            android:id="@+id/tv8"
                            android:layout_below="@+id/tv7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/textview14"
                            android:padding="2dp"
                            android:textSize="15sp"
                            />

                        <TextView
                            android:id="@+id/tv9"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/textview15"
                            android:textColor="#000"
                            android:layout_marginLeft="250dp"
                            android:padding="10dp"
                            android:textSize="15sp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentEnd="true" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_tick"
                            android:layout_marginLeft="330dp"/>
                        </RelativeLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/rel3"
                        android:background="#000"
                        android:layout_marginTop="5dp"/>
                    <RelativeLayout
                        android:id="@+id/rel4"
                        android:layout_below="@+id/rel2"
                        android:layout_marginTop="10dp"
                        android:textColor="#000"
                        android:layout_margin="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/tv10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="2dp"
                            android:textSize="18sp"
                            android:text="@string/textview13"
                            android:textColor="#000"
                            />

                        <TextView
                            android:id="@+id/tv11"
                            android:textSize="15sp"
                            android:layout_below="@+id/tv10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/textview14"
                            android:padding="2dp"
                            />

                        <TextView
                            android:id="@+id/tv12"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/textview15"
                            android:textSize="15sp"
                            android:textColor="#000"
                            android:layout_marginLeft="250dp"
                            android:padding="10dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentEnd="true" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_tick"
                            android:layout_marginLeft="330dp"/>

                    </RelativeLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/rel3"
                        android:background="#000"
                        android:layout_marginTop="5dp"/>

                    <RelativeLayout
                        android:id="@+id/rel5"
                        android:layout_below="@+id/rel4"
                        android:layout_marginTop="10dp"
                        android:layout_margin="5dp"
                        android:textColor="#000"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/tv13"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="2dp"
                            android:textSize="18sp"
                            android:text="@string/textview16"
                            android:textColor="#000"
                            />

                        <TextView
                            android:id="@+id/tv14"
                            android:textSize="15sp"
                            android:layout_below="@+id/tv13"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/textview17"
                            android:padding="2dp"
                            />

                        <TextView
                            android:id="@+id/tv15"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/textview18"
                            android:textSize="15sp"
                            android:textColor="#000"
                            android:layout_marginLeft="250dp"
                            android:padding="10dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentEnd="true" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_tick"
                            android:layout_marginLeft="330dp"/>

                    </RelativeLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/rel"
                        android:background="#000"
                        android:layout_marginTop="5dp"/>

                    <RelativeLayout
                        android:id="@+id/rel6"
                        android:layout_below="@+id/rel5"
                        android:layout_marginTop="10dp"
                        android:textColor="#000"
                        android:layout_margin="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/tv16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="2dp"
                            android:textSize="18sp"
                            android:text="@string/textview16"
                            android:textColor="#000"
                            />

                        <TextView
                            android:id="@+id/tv17"
                            android:textSize="15sp"
                            android:layout_below="@+id/tv16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/textview19"
                            android:padding="2dp"
                            />

                        <TextView
                            android:id="@+id/tv18"
                            android:textSize="15sp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/textview20"
                            android:textColor="#000"
                            android:layout_marginLeft="250dp"
                            android:padding="10dp"
                            android:layout_centerVertical="true"
                            android:layout_alignParentEnd="true" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_tick"
                            android:layout_marginLeft="330dp"/>

                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/rel7"
                        android:background="#000"
                        android:layout_marginTop="5dp"/>





                </LinearLayout>

            </FrameLayout>

        </LinearLayout>
        </ScrollView>

        <android.support.design.widget.NavigationView
            android:id="@+id/navigation_view"
            android:layout_height="match_parent"
            android:layout_width="wrap_content"
            android:layout_gravity="start"
            app:headerLayout="@layout/header"
            app:menu="@menu/drawer"

            />
    </android.support.v4.widget.DrawerLayout>


</LinearLayout>